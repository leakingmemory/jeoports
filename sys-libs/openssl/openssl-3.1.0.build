{
	"distfiles": {
		"openssl-3.1.0.tar.gz": {
			"src": [
				"https://www.openssl.org/source/openssl-3.1.0.tar.gz"
			]
		}
	},
	"builddir": "openssl-3.1.0",
	"bdep": [
		"dev-util/perl",
		"dev-util/llvm+clang",
		"app-shells/bash",
		"sys-utils/coreutils"
	],
	"configure": {
		"parameters": [
			"--openssldir=/etc/ssl"
		]
	},
	"nosysroot": {
		"env": {
			"CC": "sys-clang",
			"CXX": "sys-clang++"
		}
	}
}
