{
	"distfiles": {
		"ncurses-6.4.tar.gz" : {
			"src": [
				"https://ftp.gnu.org/gnu/ncurses/ncurses-6.4.tar.gz"
			]
		}
	},
	"builddir": "ncurses-6.4",
	"prefix": "/",
	"configure": {
		"parameters": [
			"--includedir=/usr/include",
			"--with-shared",
			"--with-termlib",
			"--with-pkg-config-libdir=/usr/lib/pkgconfig",
			"--enable-pc-files"
		]
	},
	"rdep": [
		"sys-libs/musl"
	]
}
