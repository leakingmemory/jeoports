{
	"distfiles": {
		"curl-8.0.1.tar.gz": {
			"src": [
				"https://curl.se/download/curl-8.0.1.tar.gz"
			]
		}
	},
	"builddir": "curl-8.0.1",
	"configure": {
		"parameters": [
			"--with-openssl",
			"--with-ca-bundle=/etc/ssl/ca-bundle.crt"
		]
	},
	"nosysroot": {
		"env": {
			"LIBS": " -lnghttp2 "
		}
	},
	"bdep": [
		"dev-util/llvm+clang",
		"sys-apps/diffutils"
	]
}
